    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "7.18.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.4",
    "@types/convict": "^6.1.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/ioredis": "^5.0.0",
